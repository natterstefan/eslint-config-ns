{
  "name": "eslint-config-ns",
  "version": "0.5.0",
  "description": "eslint config ready to be used in multiple projects",
  "keywords": [
    "eslint",
    "eslint-config",
    "prettier",
    "prettier-config"
  ],
  "main": "index.js",
  "files": [
    "index.js",
    "prettier.js",
    "prettier.config.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/natterstefan/eslint-config-ns"
  },
  "scripts": {
    "start": "cd example && npm start",
    "test": "(cd example && npm run lint && npm t)"
  },
  "author": "Stefan Natter",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/natterstefan/eslint-config-ns/issues"
  },
  "homepage": "https://github.com/natterstefan/eslint-config-ns#readme",
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "peerDependencies": {
    "babel-eslint": ">=10.0.3 <11",
    "eslint": ">=6.5.1 <7",
    "eslint-config-airbnb": ">=18.0.1 <19",
    "eslint-config-prettier": ">=6.4.0 <7",
    "eslint-plugin-import": ">=2.18.2 <3",
    "eslint-plugin-jest": ">=22.19.0 <23",
    "eslint-plugin-jsx-a11y": ">=6.2.3 <7",
    "eslint-plugin-prettier": ">=3.1.1 <4",
    "eslint-plugin-react": ">=7.16.0 <8",
    "eslint-plugin-react-hooks": ">=2.1.2 <3",
    "prettier": ">=1.18.2 <2"
  },
  "devDependencies": {
    "babel-eslint": "10.0.3",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "6.5.1",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "6.4.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "22.19.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "7.16.0",
    "eslint-plugin-react-hooks": "2.1.2",
    "husky": "^3.0.9",
    "prettier": "1.18.2"
  }
}
