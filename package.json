{
  "name": "eslint-config-ns",
  "version": "0.5.0",
  "description": "eslint config ready to be used in multiple projects",
  "keywords": [
    "eslint",
    "eslint-config",
    "prettier",
    "prettier-config"
  ],
  "main": "index.js",
  "files": [
    "index.js",
    "prettier.js",
    "prettier.config.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/natterstefan/eslint-config-ns"
  },
  "scripts": {
    "start": "cd example && npm start",
    "test": "(cd example && npm run lint && npm t)"
  },
  "author": "Stefan Natter",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/natterstefan/eslint-config-ns/issues"
  },
  "homepage": "https://github.com/natterstefan/eslint-config-ns#readme",
  "peerDependencies": {
    "babel-eslint": ">=10.0.2 <11",
    "eslint": ">=5.16.0 <6",
    "eslint-config-airbnb": ">=17.1.1 <18",
    "eslint-config-prettier": ">=5.1.0 <6",
    "eslint-plugin-import": ">=2.18.2 <3",
    "eslint-plugin-jest": ">=22.12.0 <23",
    "eslint-plugin-jsx-a11y": ">=6.2.3 <7",
    "eslint-plugin-prettier": ">=3.1.0 <4",
    "eslint-plugin-react": ">=7.14.2 <8",
    "eslint-plugin-react-hooks": ">=1.6.1 <2",
    "prettier": ">=1.18.2 <2"
  },
  "devDependencies": {
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.1",
    "husky": "^3.0.1"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
