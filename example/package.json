{
  "name": "example-for-eslint-confing-ns",
  "private": true,
  "version": "0.2.0",
  "description": "Example code for eslint-config-ns",
  "main": "src/index.js",
  "scripts": {
    "coverage": "jest --coverage",
    "lint": "eslint ./src",
    "lint-staged-js": "git diff --diff-filter=d --cached --name-only -z -- '*.js' '*.jsx' | xargs -0 -I % sh -c 'git show \":%\" | eslint --stdin --stdin-filename \"%\";'",
    "start": "webpack-dev-server --progress --colors",
    "test": "jest ./src",
    "watch-test": "jest --watch"
  },
  "author": "Stefan Natter",
  "license": "MIT",
  "lint-staged": {
    "*.js": [
      "npm run lint-staged-js",
      "git update-index --again",
      "jest --findRelatedTests"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-ns": "file:..",
    "eslint-config-prettier": "^5.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.12.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "husky": "^3.0.1",
    "jest": "^24.8.0",
    "jest-enzyme": "^7.0.2",
    "lint-staged": "^9.2.0",
    "prettier": "^1.18.2",
    "webpack": "^4.36.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  }
}
